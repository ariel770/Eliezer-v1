<%- include ("../partials/header")%>

<div class="container">

    <div style="width:70%; margin: 100px auto;padding: 4px;text-align: center;">
        <h1 style="text-align: center;"><strong>הוספת סוכן</strong></h1>
        <form action="/register" autocomplete="off" method="POST">
            <div class="form-group">
                <input type="text" id="username" class="form-control" name="username" placeholder="שם פרטי ומשפחה">
            </div>
            <div class="form-group">
                <input type="password" id="password" class="form-control" name="password" placeholder="סיסמא">
            </div>


            <div class="form-group">
                <input type="text" class="form-control" name="image" id="image" placeholder="הוסף  תמונה ככתובת URL">
            </div>

            <div class="form-group">
                <input type="text" id="phone" class="form-control" name="contact[phone]" placeholder="טלפון /פלאפון">
            </div>
            <div class="form-group">
                <input type="text" id="mail" class="form-control" name="contact[email]" placeholder="כתובת מייל">
            </div>


            <div class="form-group">
                <button class="btn btn-lg btn-primary btn-block ">הוסף איש קשר </button>
            </div>

        </form>

    </div>
</div>
</div>
<%- include ("../partials/footer")%>