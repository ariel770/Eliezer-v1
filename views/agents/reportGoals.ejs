<%- include ("../partials/header")%>


<div class="container">

    <div class="row">

        <h1 style="text-align: center;">יעדים חודשיים </h1>
        <div style="text-align: center;">
            <%=console.log(agents.username)%>
            <form action="/agent/<%=agents.id%>/reportGoals" method="POST">

                <table>
                    <thead>
                        <tr>

                            <th colspan="2"></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>פגישות </td>
                            <td><input type="number" class="form-control" name="meeting" required></td>
                        </tr>
                        <tr>
                            <td>הדבקת פליירים </td>
                            <td><input type="number" class="form-control" name="stickerFlyers"></td>
                        </tr>
                        <tr>
                            <td>למידה והתחדשות</td>
                            <td><input type="number" class="form-control" name="learninGandRenewal"></td>
                        </tr>
                        <tr>
                            <td>משא ומתן בתהליך </td>
                            <td><input type="number" class="form-control" name="negotiationsInTheProcess"></td>
                        </tr>
                        <tr>
                            <td>עסקאות בפועל </td>
                            <td><input type="number" class="form-control" name="actualTransactions"></td>
                        </tr>
                        <tr>
                            <td>סיורי שוכרים </td>
                            <td><input type="number" class="form-control" name="rentalTours"></td>
                        </tr>
                        <tr>
                            <td>שתופי פעולה </td>
                            <td><input type="number" class="form-control" name="collaborations"></td>
                        </tr>
                        <tr>
                            <td>שיחות ללקוחות קודמים </td>
                            <td><input type="number" class="form-control" required
                                    name="conversationsWithPreviousClients"></td>
                        </tr>
                        <tr>
                            <td>הצעות מחיר </td>
                            <td><input type="number" class="form-control" name="pricesOffer"></td>
                        </tr>
                    </tbody>
                </table>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block ">שלח טופס</button>
                </div>
            </form>
        </div>
    </div>
</div>

<%- include ("../partials/footer")%>