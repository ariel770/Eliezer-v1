<%- include ("../partials/header")%>
<div class="container">
    <div class="row">
        <div style="text-align: center;">
            <form action="/agent/<%=agents.id%>/setgoals" method="POST">
                <strong><%=agents.username%> בחר יעד עבור </strong>
                <p></p>
                <label for="money">איזה סכום תרצה להרוויח (בחישוב שנתי )</label>
                <div class="form-group">
                    <input type="number" id="money" class="form-control" name="money" placeholder="הקש סכום">
                </div>
                <label for="status"> בחר סטטוס</label>
                <div class="form-group">
                    <input type="text" list="status" multiple class="form-control" name="status"
                        placeholder="בחר סטטוס">
                    <datalist id="status">
                        <option value="sales">מכירות </option>
                        <option value="rents">השכרות </option>
                        <option value="comersial">מסחרי </option>
                    </datalist>
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block ">ערוך יעדים </button>
                </div>



            </form>
        </div>
    </div>
</div>
<%- include ("../partials/footer")%>